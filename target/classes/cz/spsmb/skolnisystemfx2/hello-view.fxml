<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1200.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.spsmb.skolnisystemfx2.controllers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <SplitPane dividerPositions="0.75" prefHeight="720.0" prefWidth="1200.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <ListView fx:id="listview" fixedCellSize="35.0" prefHeight="720.0" prefWidth="865.0" />
               </children></AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <Button fx:id="newButton" mnemonicParsing="false" onMouseClicked="#newClicked" prefHeight="25.0" prefWidth="286.0" text="Add new" />
                  <Button fx:id="editButton" layoutY="25.0" mnemonicParsing="false" onMouseClicked="#editOnClick" prefHeight="25.0" prefWidth="286.0" text="Edit" />
                  <Button fx:id="deleteButton" layoutY="50.0" mnemonicParsing="false" onMouseClicked="#deleteOnClick" prefHeight="25.0" prefWidth="286.0" text="Delete" />
               </children></AnchorPane>
        </items>
      </SplitPane>
   </children>
</VBox>
